{
  "name": "Goto this",
  "description": "跳转到选中的链接\nhttps://github.com/tanyiqu/goto-this",
  "version": "1.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "extensions.png"
  },
  "icons": {
    "16": "extensions.png",
    "48": "extensions.png",
    "128": "extensions.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "contextMenus"
  ]
}